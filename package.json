{
  "name": "@arcgis/cli",
  "version": "4.12.0",
  "description": "",
  "main": "dist/index.js",
  "bin": {
    "arcgis": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esri/arcgis-js-cli.git"
  },
  "keywords": [
    "esri",
    "arcgis",
    "cli"
  ],
  "scripts": {
    "prepare": "npm run -s build",
    "clean": "rimraf dist",
    "test": "npm run clean && jest --coverage",
    "build": "npm run clean && npm run flow && babel src -d dist -D --ignore '*.test.js,**/__mocks__/'",
    "dev": "npm run clean && npm run build && jest --watch",
    "flow": "flow",
    "pretty": "prettier 'src/**/*.js' --write"
  },
  "files": [
    "dist",
    "templates"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "author": "Rene Rubalcava <rrubalcava@esri.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.0.1",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-flowtype": "^3.6.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "flow-bin": "^0.102.0",
    "husky": "^3.0.0",
    "jest-cli": "^24.7.1",
    "lint-staged": "^9.2.0",
    "prettier": "^1.13.7",
    "prettier-eslint": "^9.0.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "command-exists": "^1.2.2",
    "cross-spawn-promise": "^0.10.1",
    "del": "^5.0.0",
    "download-git-repo": "^2.0.0",
    "fs-extra": "^8.1.0",
    "fs.promised": "^3.0.0",
    "inquirer": "^6.5.0",
    "live-server": "^1.2.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.startcase": "^4.4.0",
    "mkdirp": "^0.5.1",
    "ora": "^3.4.0",
    "pkg-dir": "^4.1.0",
    "promise-polyfill": "^8.1.0",
    "promisify-node": "^0.5.0",
    "prompt": "^1.0.0",
    "ramda": "^0.26.1",
    "recursive-copy": "^2.0.8",
